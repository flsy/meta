{
  "name": "metatable",
  "version": "0.0.2",
  "private": false,
  "description": "meta table",
  "main": "lib/exports",
  "typings": "lib/exports",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "testtodo": "react-scripts test",
    "test": "echo todo",
    "eject": "react-scripts eject",
    "transpile": "rm -rf ./lib && tsc -p tsconfiglib.json",
    "prepare": "npm run transpile",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\"",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "verify": "npm run format && npm run lint && npm run test && npm run transpile"
  },
  "dependencies": {
    "react": "^16.13.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.5.0",
    "@testing-library/user-event": "7.2.1",
    "@types/jest": "24.9.1",
    "@types/node": "12.12.29",
    "@types/react": "16.9.23",
    "@types/react-dom": "16.9.5",
    "react-dom": "16.13.0",
    "react-scripts": "3.4.0",
    "typescript": "3.7.5",
    "eslint": "6.8.0",
    "prettier": "1.19.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
